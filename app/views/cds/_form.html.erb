<%= simple_form_for(@cd) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :grupo %>
    <%= f.input :titulo %>
    <%= f.input :descripcion %>
    <%= f.input :enlace %>
    <%= f.input :fecha %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
