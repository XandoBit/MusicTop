

<h1>Sign up</h1>
<%= form_for @user, url:{action:"create"} do |f| %>
 <%= f.label :nombre %>
  <%= f.text_field :nombre %>
<br>
   <%= f.label :email %>
  <%= f.text_field :email %>
<br>
   <%= f.label :pass %>
  <%= f.text_field :pass %>
<br>
   <%= f.label :password_digest %>
  <%= f.text_field :password_digest %>

  <%= f.submit"Crear nueva cuenta en MuiscTop" %>
  <%end%>