
<h1>Sign up</h1>

<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= f.label :nombre %>
      <%= f.string_field :nombre %>

      <%= f.label :email %>
      <%= f.string_field :email %>

      <%= f.label :pass %>
      <%= f.string_field :pass %>

   

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>